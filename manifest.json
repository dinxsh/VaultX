{
  "manifest_version": 3,
  "name": "VaultX - PedalsUp Utility",
  "version": "1.0",
  "description": "Universal wallet providing seamless connectivity for all your React applications.",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "VaultX",
    "default_popup": "pedalsup.html"
  },
  "permissions": ["storage"]
}
